{
  "dbName": "NS_DB",
  "version": "1.0",
  "objectStores": [
    {
      "name": "pre-task",
      "keyPath": {
        "keys": ["questionId"]
      },
      "indexes": ["questionId"]
    },
    {
      "name": "main-task",
      "keyPath": {
        "keys": ["topicId", "snippetNumber"]
      },
      "indexes": [
        "topicId",
        "snippetNumber",
        "snippetTitle",
        "snippetText",
        "snippetNutriscore",
        "snippetUserScore"
      ]
    },
    {
      "name": "post-task",
      "keyPath": {
        "keys": ["questionId"]
      },
      "indexes": ["questionId"]
    }
  ]
}
