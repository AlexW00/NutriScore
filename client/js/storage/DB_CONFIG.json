{
  "dbName": "NS_DB",
  "version": "1.0",
  "objectStores": [
    {
      "name": "pre-task",
      "keyPath": {
        "keys": ["questionId"]
      },
      "indexes": ["questionId"]
    },
    {
      "name": "main-task-topics",
      "keyPath": {
        "keys": ["topicId"]
      },
      "indexes": ["topicId", "snippetNumber", "hasCrediScore"]
    },
    {
      "name": "main-task-snippets",
      "keyPath": {
        "keys": ["topicId", "snippetNumber"]
      },
      "indexes": [
        "topicId",
        "snippetNumber",
        "snippetTitle",
        "snippetText",
        "snippetCrediScore"
      ]
    },
    {
      "name": "main-task-snippet-ratings",
      "keyPath": {
        "keys": ["topicId", "snippetNumber"]
      },
      "indexes": ["topicId", "snippetNumber", "snippetRating"]
    },
    {
      "name": "post-task",
      "keyPath": {
        "keys": ["questionId"]
      },
      "indexes": ["questionId"]
    }
  ]
}
